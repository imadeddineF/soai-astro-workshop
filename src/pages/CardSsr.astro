---
import Layout from "../layouts/Layout.astro";
export const prerender = false;

type Post = {
  userId: number;
  id: number;
  title: string;
  body: string;
}

const req = await fetch('https://jsonplaceholder.typicode.com/posts/1');
const post: Post = await req.json();

console.log("post data:",post);

---

<Layout title="Card">
  <div>
    <h1>SSR page</h1>
    <p>{post.id}</p>
    <p>{post.title}</p>
    <p>{post.body}</p>
  </div>
</Layout>